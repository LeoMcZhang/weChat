/**index.wxss**/
swiper,swiper image{
  width: 750rpx;
  height:290rpx;
}
.title_wrap {
  margin: 20rpx 10rpx;
  font-size: 26rpx;
  line-height: 36rex;
}
.title_wrap text {
  margin: 0 5px;
}
.title_wrap image {
  width: 36rpx;
  height: 36rpx;
  display: block;
}
.scroll_wrap {
  height: 350rpx;
}
.scroll_wrap .cont_wrap image {
  margin-bottom: 8px;
}
.cont_wrap {
  display: flex;
  margin: 0 10rpx;
  flex-wrap: wrap;
  space-between;
}
.cont_wrap .cont_block {
  width: 33.3%;
  box-sizing: border-box;
}
.cont_wrap .cont_block image {
  width: 230rpx;
  height: 230rpx;
  display: block;
}
.cont_wrap .cont_block .cont_text {
  color: #666;
  line-height: 30rpx;
  font-size: 28rpx;
  margin: 8px 0;
}
